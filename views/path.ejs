<!DOCTYPE html>
<html lang="en">
<head>
	
	<%- include(partial("head")) %>

</head>
<body>
	
	<div class="app">
		
		<%- include(partial("menu")) %>
		
		<div class="section main_section">

			<h1>Analytics &mdash; <code><%- path %></code></h1>

			<a href="analytics">&leftarrow; Back</a>
		
			<h2>This page has been viewed <code><%- db.getViewsForPath(path) %></code> time(s).</h2>

			<div>

				<div>

					<h2>Countries</h2>

					<div class="list">

						<% const co = db.getCountriesObjectForPath(path); %>
						
						<% for (const country of Object.keys(co).sort((a, b) => co[b] - co[a])) { %>

							<div>

								<h3><%- country %> <code style="float: right;"><%- co[country] %></code></h3>

							</adiv>

						<% } %>

					</div>

				</div>

			</div>

		</div>

	</div>

</body>
</html>
